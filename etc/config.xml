<?xml version="1.0"?>
<config
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd"
>
    <default>
        <payment>
            <webbhuset_sveacheckout>
                <debug>0</debug>
                <active>0</active>
                <model>SveaCheckoutFacade</model>
                <order_status>pending</order_status>
                <title>Svea Ekonomi Checkout</title>
                <allowspecific>0</allowspecific>
                <group>svea</group>
                <can_edit>false</can_edit>
                <can_authorize>true</can_authorize>
                <can_void>true</can_void>
                <can_cancel>true</can_cancel>
                <can_capture>true</can_capture>
                <can_capture_partial>true</can_capture_partial>
                <can_refund>true</can_refund>
                <can_refund_partial>true</can_refund_partial>
                <can_refund_partial_per_invoice>1</can_refund_partial_per_invoice>
                <can_use_checkout>true</can_use_checkout>
                <can_use_internal>false</can_use_internal>
                <can_use_for_multishipping>false</can_use_for_multishipping>
                <debug_mode>false</debug_mode>
                <merchant_id backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <shared_secret backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <is_gateway>1</is_gateway>
                <allowed_customers>0</allowed_customers>
                <override_terms_url>0</override_terms_url>
                <enable_international_purchases>0</enable_international_purchases>
                <prefer_simple_integration>0</prefer_simple_integration>
            </webbhuset_sveacheckout>
        </payment>
    </default>
</config>
